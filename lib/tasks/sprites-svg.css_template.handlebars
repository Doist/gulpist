.svg_icon {
    background-image: url("/static/apps/landing_pages/{{ svgPath }}");
    background-repeat: no-repeat;
}

{{#each this.items}}
.svg_icon--{{ filename }} {
    background-position: {{unit x 'px' 16 -1}} {{unit y 'px' 16 -1}};
    height: {{ cssHeight }}px;
    width: {{ cssWidth }}px;
}
{{/each}}
